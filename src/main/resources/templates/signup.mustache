<!DOCTYPE html>
<html lang="en">

{{> head-partial}}

<body>

  <!-- Navigation -->
  {{> navbar-partial}}

  <!-- Page Content -->
  <div class="container">

    <div class="container">
      <form name="signupData" action="/signup" method="post">
        <div class="row">
          <h4>Signup</h4>
        </div>
        {{> credentials-inputs}}
        {{> address-inputs}}
        <input type="hidden" name="_csrf" value="{{_csrf.token}}" />
        <button id="sign-up" class="btn btn-primary" type="submit">Signup</button>
      </form>
    </div>
    <!-- /.row -->

  </div>
  <!-- /.container -->

  {{> footer-scripts-partial}}

</body>

</html>
